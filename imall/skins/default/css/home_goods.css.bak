.imcs-combo-container. goods-thumb { background-color: #FFF; width:100px; height: 100px;}
.imcs-bundling-container .goods-thumb a,
.imcs-combo-container .goods-thumb a { line-height: 0; background-color: #FFF; text-align: center; vertical-align: middle; display: table-cell; *display: block; width: 100px; height: 100px; overflow: hidden;}
.imcs-bundling-container .goods-thumb img,
.imcs-combo-container .goods-thumb img { max-width: 100px; max-height: 100px; margin-top:expression(100-this.height/2); *margin-top:expression(50-this.height/2);}
.imcs-bundling-container dl { display:block; padding: 0; margin: 4px 0 0 0;}
.imcs-bundling-container dl dt { line-height: 24px; text-overflow: ellipsis; white-space: nowrap; width: 100px; height: 24px; overflow:hidden;}
.imcs-bundling-container dl dt a { color: #555;}
.imcs-bundling-container dl dd { line-height: 16px; color: #999; white-space: nowrap; text-overflow: ellipsis; width: 100px; overflow: hidden;}
.imcs-bundling-container dl dd .o-price { text-decoration: line-through;}
.imcs-bundling-container dl dd .b-price { font-weight: 600; color: #D93600;}
.imcs-bundling-price { color: #777; background: url(../images/shop/bundling.gif) no-repeat 0 -220px; display: block; width: 130px; float: left; padding: 0 0 0 70px; margin: 15px 0 0 0;}
.imcs-bundling-price dl { color: #999; }
.imcs-bundling-price dt { font-weight: 600;}
.imcs-bundling-price dd { }
.imcs-combo-container,
.combo-goods-list ul { font-size: 0; *word-spacing:-1px/*IE6、7*/;}
.imcs-combo-container { height: 194px;}
.imcs-combo-container .default-goods,
.imcs-combo-container .combo-goods-list,
.imcs-combo-container .combo-price,
.combo-goods-list li { font-size: 12px; vertical-align: top; letter-spacing: normal; word-spacing: normal; display: inline-block; *display: inline/*IE7*/; width: 100px; *zoom: 1; }
.imcs-combo-container .default-goods { background: url(../images/shop/bundling.gif) no-repeat 100px 0 ; padding: 0 60px 0 0; margin-top: 15px;}
.combo-goods-list li { background: url(../images/shop/bundling.gif) no-repeat 0 0 ; padding: 0 0 0 60px;}
.combo-goods-list .combo-goods-first { position: -60px 0; padding: 0;}
.imcs-combo-container .combo-goods-list { width: 580px; margin-top: 15px; overflow-x: scroll;}
.combo-goods-list ul { white-space: nowrap;}
.imcs-combo-container dl { display: block; width: 100px; margin-top: 8px;}
.imcs-combo-container dl dt { line-height: 16px; white-space: normal; display: block; height: 32px; margin-bottom: 2px; overflow: hidden;}
.imcs-combo-container dl dd { line-height: 20px; font-weight: 600; color: #D93600;}
.imcs-combo-container dl dd .checkbox { vertical-align: middle; margin-right: 4px;}
.imcs-combo-container .combo-price { color: #777; background: url(../images/shop/bundling.gif) no-repeat 0 -220px; width: 150px; padding: 0 0 0 70px; margin: 15px 0 0 0;}
.imcs-combo-container .combo-price dl { width: 150px;}
.imcs-combo-container .combo-price dd { font-weight: normal; color: #999; }

/* 商品内容处TabBar */
