/*规格值的选择*/
.imcs-key ul { font-size: 0; *word-spacing:-1px/*IE6、7*/;}
.imcs-key ul li { vertical-align: top; letter-spacing: normal; word-spacing: normal; display: inline-block; margin-right: 6px; position: relative; z-index: 1;}
.imcs-key ul li { *display: inline/*IE6,7*/; *zoom:1;}
.imcs-key ul li a { white-space: nowrap; display: block; min-height: 24px; padding: 1px; border: 1px solid #DDD; cursor: pointer;}
.imcs-key ul li.sp-img a { background-color: #FFF; font-size: 12px; color: #999; padding-right: 5px;}
.imcs-key ul li.sp-img a img { vertical-align: middle; display: inline-block; max-width: 24px; max-height: 24px; margin-right: 5px;}
.imcs-key ul li.sp-txt a { font-size: 12px; line-height: 24px; color: #000; text-decoration: none; background: #FFF none; text-align: center; white-space: nowrap; min-width: 20px; height: 24px; padding: 0 5px !important; margin: 1px;}
.imcs-key ul li.sp-img a:hover, 
.imcs-key ul li.sp-img a.hovered { color: #D93600; text-decoration: none; border: 2px solid #D93600; padding: 0 4px 0 0;}
