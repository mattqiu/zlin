.imcs-handle a.like i {background-position: 0px -32px;}
.imcs-handle a.inform i {background-position: 0px -48px;}

.imcs-handle a.compare { float: right;}
.imcs-handle a.compare i {background-position: 0px -64px;}
.imcs-handle a.compare.selected i {background-position: 0px -80px;}

/* 物流运费 */
.imcs-freight dt { position:relative; z-index: 80;}
.imcs-freight dt .imcs-freight-box { display:none;}
.imcs-freight dt:hover .imcs-freight-box { display:block; border: solid 2px #F90; width: 420px; background:#FFF; padding: 6px; position:absolute; z-index: 1; top: 28px; left: 12px; border-radius: 4px;  box-shadow:3px 3px 3px rgba(0,0,0,.3)}
.imcs-freight dt:hover .imcs-freight-box a { background:none; text-align:center; line-height: 24px; height: 24px; width:60px; float:left; padding:0; margin:0}
.imcs-freight dt:hover .imcs-freight-box a:hover { text-decoration: none; color: #FFF; background-color: #F90; border-radius: 4px;}
.imcs-freight dt a { background:url(../images/shop/public.png) no-repeat 100% -103px; padding: 0 11px 0 1px;}
.imcs-freight dd span { margin-right: 10px;}
.imcs-freight dd span em { padding: 0 2px;}

/* 商品已下架状态提示 */
