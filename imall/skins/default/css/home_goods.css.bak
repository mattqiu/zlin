.imcs-comment .rate strong { font: lighter 40px/40px arial; vertical-align: bottom;}
.imcs-comment .rate sub { font: 16px/20px arial; vertical-align: bottom; margin-right: 6px;}
.imcs-comment .rate span { color: #999; display: block; clear: both;}
.imcs-comment .percent { vertical-align: middle; display: inline-block; *display: inline; *zoom:1;}
.imcs-comment .percent dl { font-size: 0; *word-spacing:-1px/*IE6、7*/;}
.imcs-comment .percent dt { font-size: 12px; line-height: 20px; vertical-align: top; letter-spacing: normal; word-spacing: normal; display: inline-block; width: 80px; height: 20px;}
.imcs-comment .percent dt { *display: inline/*IE6,7*/;}
.imcs-comment .percent dt em { color: #999; margin-left: 4px;}
.imcs-comment .percent dd { background-color: #F5F5F5; vertical-align: top; letter-spacing: normal; word-spacing: normal; display: inline-block; width: 100px; height: 14px; margin: 3px 0;}
