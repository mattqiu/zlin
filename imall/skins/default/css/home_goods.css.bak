.imcs-plus .goods-gift .goods-gift-thumb span img { max-width: 24px; max-height: 24px; margin-top:expression(24-this.height/2); *margin-top:expression(12-this.height/2);}
.imcs-plus .goods-gift .goods-gift-name { color: #005EA6; margin-left: 5px;}
.imcs-plus .goods-gift ul li em { font-family: Arial; color: #F60; margin-left: 5px;}
.imcs-key { border-top: dotted 1px #DDD; padding-top: 15px; }
.imcs-meta dl, .imcs-plus dl, .imcs-key dl { font-size: 0; *word-spacing:-1px/*IE6、7*/;}
.imcs-plus dl dt, .imcs-plus dl dd,
.imcs-meta dl dt, .imcs-meta dl dd,
.imcs-key dl dt, .imcs-key dl dd { font-size: 12px; line-height: 20px; vertical-align: top; letter-spacing: normal; word-spacing: normal; display: inline-block; *display: inline/* IE6,7 */; min-height: 20px; padding: 4px 0; *zoom:1;}

.imcs-meta dl dt, .imcs-plus dl dt, .imcs-key dl dt { font-family: simsun; text-align: right; width: 96px; margin-right: 6px;}
.imcs-meta dl dd, .imcs-plus dl dd, .imcs-key dl dd { text-align: left; width: 510px; *width: 509px/*IE7*/;}
.imcs-meta dl dd i.ver-line { display: inline-block; *display: inline; zoom: 1;}
.imcs-meta dl dd a { color: #005AA0; vertical-align: top; display: inline-block; *display: inline/*IE7*/; *zoom: 1;}
.imcs-meta dl dd a:hover { text-decoration: underline;}

.imcs-meta dl dt {font: 12px/1.5 "Microsoft Yahei", Tahoma, Helvetica, Arial, sans-serif, 宋体; font-size-adjust: none; font-stretch: normal; font-size:16px;}
.imcs-meta .cost-price strong  { text-decoration: line-through;}
.imcs-meta .cost-price .rebate {background: rgb(20, 255, 20); padding: 0px 5px; height: 20px; text-align: center; color: rgb(255, 255, 255); margin-left: 25px; float:right;}
.imcs-meta .cost-price .jd-current {color: #D93600; font-family: Arial; font-size: 14px; vertical-align: -2px; float: left; margin-left: 0px; float:right;  margin-right: 116px;}

.imcs-meta .price strong  {font-size: 20px; font-weight:600; color: #D93600; vertical-align: middle; float:left; position: relative;}
.imcs-meta .price span {line-height: 20px; color: #FFF; vertical-align: middle; height: 20px; padding: 1px 3px; *padding: 2px 3px 0 3px; margin-right: 8px; float:left; position: relative;}
.imcs-meta .price em {color: #999; vertical-align: middle; margin-left: 8px; float:left; position: relative; font-family: "微软雅黑"; font-size: 14px;}
