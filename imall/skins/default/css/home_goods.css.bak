.imcs-handle a.selected {}
.imcs-handle a span { font-family: Arial; color: #AAA; margin-left: 4px;}
.imcs-handle i {background: url(../images/btn_handle.png) no-repeat; vertical-align: middle; width: 16px; height: 16px; display: inline-block; margin-right: 2px;}

.imcs-handle .bdsharebuttonbox{ color: #777; background-color: #FFF; float: left; padding: 1px 6px; margin-right: 2px;}
.imcs-handle a.share i {background-position: 0px 0px;}
