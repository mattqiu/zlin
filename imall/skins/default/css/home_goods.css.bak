.imcs-key ul li.sp-txt a.hovered { border: 2px solid #D93600; margin: 0;}
.imcs-key ul li a i { display: none;}
.imcs-key ul li a.hovered i { font-size: 0; line-height: 0; background: url(../images/shop/public_tag.png) no-repeat 0 0; display: block; width: 11px; height: 11px; position: absolute; z-index: 1; right: 2px; bottom: 2px;}
/*购买数量和库存*/
