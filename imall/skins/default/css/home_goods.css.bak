.imcs-saleout dt { font-size: 14px; line-height: 24px; font-weight: 600; color: #D93600; height: 24px; }
.imcs-saleout dt i { margin-right: 6px;}
.imcs-saleout dd { color: #777; clear: both; line-height: 20px; margin: 6px 0; padding-left: 18px;}



/* 满即送活动 */
.imcss-mansong { font-size: 0; *word-spacing:-1px/*IE6、7*/; background-color: #FFF; padding: 9px; border: solid #D7D7D7; border-width: 0 1px 1px;}
.imcss-mansong-ico { background: url(../images/shop/public_tag.png) no-repeat 0 -20px; vertical-align: top; letter-spacing: normal; word-spacing: normal; display: inline-block; *display: inline/*IE7*/; width: 80px; height: 40px; margin: 10px 20px 0 10px; *zoom:1;}
.imcss-mansong-content { vertical-align: top; letter-spacing: normal; word-spacing: normal; display: inline-block; *display: inline/*IE7*/; width: 840px; *zoom:1;}
.imcss-mansong-content dt { font: 700 14px/20px "Microsoft Yahei"; color: #555; height: 20px; margin-top: 5px;}
.imcss-mansong-content dt time { font-weight: normal; font-size: 12px; color: #999; vertical-align: middle; display:inline-block; margin-left: 4px;}
.imcss-mansong-content dd { font: normal 12px/30px "Microsoft Yahei"; margin-top: 15px}
.imcss-mansong-content dd span { color: #333; margin-right: 20px;}
.imcss-mansong-content dd span em { color: #F30; font-weight: 600; margin: 0 2px;}
.imcss-mansong-content dd span i { color: #390; font-weight: 600; margin: 0 2px;}
.imcss-mansong-content dd a { line-height: 0; background-color: #FFF; text-align: center; vertical-align: middle; display: inline-block; width: 28px; height: 28px; border: solid 1px #F5F5F5; overflow: hidden;}
.imcss-mansong-content dd img { max-width: 28px; max-height: 28px; margin-top:expression(28-this.height/2); *margin-top:expression(14-this.height/2)}
.imcss-mansong-remark { color:#999; line-height: 18px !important;}

/*优惠套餐销售*/
