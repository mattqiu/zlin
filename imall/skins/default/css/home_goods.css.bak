/* 商品信息购买 */
.imcs-detail {min-height: 460px; margin-bottom: 10px; border: solid 1px #EEE; position: relative; z-index: 2;}
.imcs-detail .imcs-info {position: absolute; z-index: 2; top: 0; right: 0;}

/* 商品图片放大镜 */
.imcs-goods-picture { background-color: #FFF; position: absolute; z-index:99; top: 0; left: 0;}
.imcs-goods-picture .gallery_wrap { width: 100%; height: 100%; position: absolute; top: 0; left: 0;} 
.imcs-goods-picture .gallery { width: 100%; height: 100%; position: absolute; top: 0; left: 0;}
.imcs-goods-picture .place_gallery { margin-left: -180px; top: 4%; left: 50%;}
.imcs-goods-picture img { display: block; -ms-interpolation-mode: bicubic;}
.imcs-goods-picture .levelB, 
.imcs-goods-picture .levelC { cursor: url(../images/shop/zoom.png), -moz-zoom-in; }
.imcs-goods-picture .levelD { cursor: url(../images/shop/hand.png), -moz-grab; }
.imcs-goods-picture .controller_wrap { filter:progid:DXImageTransform.Microsoft.gradient(enabled='true',startColorstr='#B2FFFFFF', endColorstr='#B2FFFFFF'); background:rgba(255,255,255,0.7); padding: 0; position: absolute; left: 20px; right: 20px; bottom: -42px;}
.imcs-goods-picture .controller { width: 320px; height: 52px; position: relative; overflow: hidden;}
.imcs-goods-picture .controller ul { font-size: 0; *word-spacing:-1px/*IE6、7*/; width: 100%; position: absolute; top: 0; left: 0;}
.imcs-goods-picture .controller li { vertical-align: middle; letter-spacing: normal; word-spacing: normal; display: inline-block; width: 58px;}
.imcs-goods-picture .controller li { *display: inline/*IE6,7*/;}
.imcs-goods-picture .controller a { background-color: #FFF; text-align: center; vertical-align: middle; display: table-cell; *display: block; padding: 1px; border: 1px solid #EEE; overflow: hidden;}
.imcs-goods-picture .controller a img { max-width: 48px; max-height: 48px; margin-top:expression(48-this.height/2); *margin-top:expression(24-this.height/2);}
.imcs-goods-picture .controller a:hover, .imcs-goods-picture .controller .current { padding: 0; border-width: 2px; border-color: #D93600;}
.imcs-goods-picture .prev, .imcs-goods-picture .next { filter:progid:DXImageTransform.Microsoft.gradient(enabled='true',startColorstr='#66FFFFFF', endColorstr='#66FFFFFF');background:rgba(255,255,255,0.4); display: none; border-radius: 0 56px 56px 0; width: 28px; height: 56px; position: absolute; top: -190px; left: 0;}
.imcs-goods-picture:hover .prev, .imcs-goods-picture:hover .next { display: block;}
.imcs-goods-picture .prev:hover, .imcs-goods-picture .next:hover { filter:progid:DXImageTransform.Microsoft.gradient(enabled='true',startColorstr='#CCFFFFFF', endColorstr='#CCFFFFFF');background:rgba(255,255,255,0.8);}
.imcs-goods-picture .prev span, .imcs-goods-picture .next span { text-indent: -99px; background: url(../images/shop/public_tag.png) no-repeat -12px 0; display: block; margin: 18px 0 0 4px; width: 10px; height: 20px; overflow: hidden; cursor: pointer;}
.imcs-goods-picture .hide, .imcs-goods-picture:hover .hide { display: none;}
.imcs-goods-picture .next { border-radius: 56px 0 0 56px; left: auto; right: 0;}
.imcs-goods-picture .next span { background-position: -22px 0; margin-left: 9px;}
.imcs-goods-picture .close_wrap { position: absolute; top: -10px; right: -10px;}
.imcs-goods-picture .close_wrap a { font: lighter 20px/20px Verdana; color: #CCC; background-color: #FFF; text-align: center; display: none; width: 22px; height: 22px; border: solid 1px #CCC; border-radius: 24px; overflow: hidden;}
.imcs-goods-picture .close_wrap a:hover { text-decoration: none; color: #333; border-color: #333;}

/* 商品名称 */

/* product_read */
.imcs-goods-summary { background-color: #FAFAFA; float: right; width: 622px; min-height: 460px; margin-right: 208px; border-right: solid 1px #EEE; box-shadow: inset 4px 0 1px rgba(153,153,153,0.15);}
.imcs-goods-summary .name { padding: 20px 25px;}
.imcs-goods-summary .name h1, 
