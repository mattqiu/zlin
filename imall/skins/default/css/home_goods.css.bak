.imcs-goods-summary .name strong { font-weight: normal; font-size: 12px; line-height: 17px; color: #777; white-space: normal; margin-top: 5px;}
.ownshop .imcs-goods-summary { width: 830px; margin-right: 0; border: none;}
/* 销售信息 */
